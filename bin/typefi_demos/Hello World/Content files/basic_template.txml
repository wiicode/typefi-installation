    <tps:definition tag="elementVariantDef">
        <tps:elementVariantDef id="1">
            <tps:elementFamily>float</tps:elementFamily>
            <tps:pos x="12.7 mm" y="12.7 mm"/>
            <tps:size width="47.3 mm" height="7.05833 mm"/>
            <tps:rules>
                <tps:rule tag="placementRule" type="pageSide" priority="1">
                    <tps:placementRule pageSide="left"/>
                </tps:rule>
                <tps:rule tag="placementRule" type="pageSide" priority="1">
                    <tps:placementRule pageSide="right"/>
                </tps:rule>
                <tps:rule tag="floatFrameAlignmentRule" type="alignment" priority="0">
                    <tps:floatFrameAlignmentRule xRef="left" yRef="top" xBase="variant" yBase="textFrame" topLimit="textFrame" floatXRef="left" floatYRef="top" leftLimit="none" rightLimit="none" bottomLimit="textFrame"/>
                </tps:rule>
                <tps:rule tag="floatFrameAlignmentRule" type="alignment" priority="0">
                    <tps:floatFrameAlignmentRule xRef="left" yRef="middle" xBase="variant" yBase="textFrame" topLimit="textFrame" floatXRef="left" floatYRef="middle" leftLimit="none" rightLimit="none" bottomLimit="textFrame"/>
                </tps:rule>
                <tps:rule tag="floatFrameAlignmentRule" type="alignment" priority="0">
                    <tps:floatFrameAlignmentRule xRef="left" yRef="bottom" xBase="variant" yBase="textFrame" topLimit="textFrame" floatXRef="left" floatYRef="bottom" leftLimit="none" rightLimit="none" bottomLimit="textFrame"/>
                </tps:rule>
                <tps:rule tag="floatLineAlignmentRule" type="alignment" priority="4">
                    <tps:floatLineAlignmentRule xRef="right" xBase="pageMargin" yBase="baseline" xOffset="-3 mm" topLimit="textFrame" floatXRef="right" floatYRef="top" leftLimit="none" rightLimit="none" bottomLimit="textFrame"/>
                </tps:rule>
            </tps:rules>
            <tps:frames>
                <tps:frame type="contentFrame"/>
            </tps:frames>
            <tps:floatSpacing top="0 mm" left="0 mm" right="0 mm" bottom="0 mm"/>
        </tps:elementVariantDef>
    </tps:definition>
    
    
